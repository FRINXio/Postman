										"exec": [
											"console.log(\"Response: \" + responseBody)",
											"",
											"pm.test(\"Status code is 200\", function () {",
											" pm.response.to.have.status(200);",
											"});",
											"",
											"    var results = [];",
											"    var result = pm.response.json()[\"output\"][\"node-results\"][\"node-result\"];",
											"    for(var i = 0; i < result.length; i++){",
											"             results[i] = result[i][\"node-id\"];",
											"    }",
											"pm.test(\"Validate node overall status\", function() {",
											"       if ( [\"output\"] ){",
											"            pm.expect(pm.response.json()[\"output\"][\"overall-status\"]).to.eql(postman.getEnvironmentVariable(\"operations_result_complete\"));",
											"        }",
											"    });",
											"pm.test(\"Validate node R1 result status\", function() {",
											"        var nodes = pm.response.json()[\"output\"][\"node-results\"][\"node-result\"];",
											"        for(var i=0;i<nodes.length;i++){",
											"            if(nodes[i][\"node-id\"] == \"R1\"){",
											"                pm.expect(nodes[i][\"status\"]).to.eql(postman.getEnvironmentVariable(\"operations_result_complete\"));",
											"                break;",
											"            }   ",
											"            ",
											"            var found = results.includes('R1');",
											"            pm.expect(found).to.be.true;",
											"       }",
											"",
											"    }); ",
											"pm.test(\"Validate node R2 result status\", function() {",
											"        var nodes = pm.response.json()[\"output\"][\"node-results\"][\"node-result\"];",
											"        for(var i=0;i<nodes.length;i++){",
											"            if(nodes[i][\"node-id\"] == \"R2\"){",
											"               pm.expect(nodes[i][\"status\"]).to.eql(postman.getEnvironmentVariable(\"operations_result_complete\"));",
											"                break;",
											"            } ",
